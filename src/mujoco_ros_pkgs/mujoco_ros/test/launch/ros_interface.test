<?xml version="1.0"?>
<launch>

  <env name="ROSCONSOLE_FORMAT" value="[${severity}] [${time}] [${logger}] [${node}]: ${message}"/>
  <env name="ROSCONSOLE_CONFIG_FILE"
       value="$(find mujoco_ros)/config/rosconsole.config"/>

  <param name="/use_sim_time" value="true"/>
  <test test-name="ros_interface_test" pkg="mujoco_ros" type="ros_interface_test" time-limit="180.0"/>
</launch>
